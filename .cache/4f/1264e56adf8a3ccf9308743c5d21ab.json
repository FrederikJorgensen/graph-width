onst value = prompt('Value:');\n\n        const splitted = value.split(',');\n        const temp = [];\n        splitted.forEach((vertix) => {\n          const v = parseInt(vertix, 10);\n          temp.push(v);\n        });\n        // d.data.vertices = temp;\n\n        d.tree.addNode(d, value, temp);\n      },\n    },\n    {\n      title: 'Delete Node',\n      action(elm, d, i) {\n        d.tree.removeNode(d);\n      },\n    },\n    {\n      title: 'Set Value',\n      action(elm, d, i) {\n        const number = prompt('Value:');\n        d.data.label = number;\n\n        const splitted = number.split(',');\n        const temp = [];\n        splitted.forEach((vertix) => {\n          const v = parseInt(vertix, 10);\n          temp.push(v);\n        });\n        d.data.vertices = temp;\n        d.tree.restart();\n      },\n    },\n  ];\n};\n"},"lineCount":null}},"error":null,"hash":"8d648a75befecae5124ea441ad7f9297","cacheData":{"env":{}}}